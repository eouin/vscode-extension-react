{
  "name": "sapux-data-modeler",
  "version": "1.0.0",
  "publisher": "SAPUX",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/eouin/vscode-extension-react"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "main": "./out/extension",
  "activationEvents": [
    "onCommand:extemsion.sapux.datamodeler"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extemsion.sapux.datamodeler",
        "title": "Go"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sapux-datamodeler",
          "title": "SAP UX",
          "icon": "media/smiley.png"
        }
      ]
    },
    "views": {
      "sapux-datamodeler": [
        {
          "id": "sapux-datamodeler-canvas",
          "name": "Data Modeler"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "extemsion.sapux.datamodeler",
          "when": "view == sapux-datamodeler-canvas",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc",
    "compile:views": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc --watch",
    "watch:views": "webpack --watch --mode development",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "yarn compile",
    "package": "vsce package"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "vscode": "^1.1.34",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "css-loader": "^2.1.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "styled-components": "^4.2.0",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "vsce": "^1.61.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
